{
  "name": "@gabrielbryk/json-schema-to-zod",
  "version": "2.15.0",
  "description": "Converts JSON schema objects or files into Zod schemas",
  "type": "module",
  "types": "./dist/types/index.d.ts",
  "bin": "./dist/cli.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:esm": "tsc -p tsconfig.build.json",
    "build": "pnpm gen && pnpm test && rimraf ./dist && pnpm build:esm",
    "prepare": "pnpm gen && rimraf ./dist && pnpm build:esm && husky",
    "dry": "pnpm build && pnpm publish --dry-run",
    "dev": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "gen": "tsx ./createIndex.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "lint": "eslint \"src/**/*.{ts,tsx}\" \"test/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" \"test/**/*.ts\" --fix",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "audit": "pnpm audit --audit-level=moderate",
    "ci": "pnpm run build && pnpm run format:check && pnpm run lint",
    "lint-staged": "lint-staged",
    "local-release": "changeset version && changeset publish",
    "release": "changeset publish",
    "prepublishOnly": "pnpm run ci",
    "smoke:esm": "pnpm build:esm && node --input-type=module -e \"import { jsonSchemaToZod } from './dist/index.js'; console.log(jsonSchemaToZod({type:'string'}));\""
  },
  "packageManager": "pnpm@10.25.0",
  "c8": {
    "exclude": [
      "dist",
      "createIndex.ts",
      "jest.config.js",
      "jest.config.ts",
      "jest.config.mjs",
      "test"
    ]
  },
  "keywords": [
    "zod",
    "json",
    "schema",
    "converter",
    "cli"
  ],
  "author": "Stefan Terdell",
  "contributors": [
    "Chen (https://github.com/werifu)",
    "Nuno Carduso (https://github.com/ncardoso-barracuda)",
    "Lars Strojny (https://github.com/lstrojny)",
    "Navtoj Chahal (https://github.com/navtoj)",
    "Ben McCann (https://github.com/benmccann)",
    "Dmitry Zakharov (https://github.com/DZakh)",
    "Michel Turpin (https://github.com/grimly)",
    "David Barratt (https://github.com/davidbarratt)",
    "pevisscher (https://github.com/pevisscher)",
    "Aidin Abedi (https://github.com/aidinabedi)",
    "Brett Zamir (https://github.com/brettz9)",
    "vForgeOne (https://github.com/vforgeone)",
    "Adrian Ordonez (https://github.com/adrianord)",
    "Jonas Reucher (https://github.com/Mantls)",
    "Nicolas Forstner (https://github.com/nlsfnr)",
    "Jordyn https://github.com/ColourfulMelon"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabrielbryk/json-schema-to-zod"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@types/jest": "^29.5.14",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "eslint": "^9.39.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.3.4",
    "tsx": "^4.1.1",
    "typescript": "^5.2.2",
    "zod": "^4.3.5"
  }
}
